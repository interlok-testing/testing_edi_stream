<adapter>
  <unique-id>edi-testing</unique-id>
  <start-up-event-imp>com.adaptris.core.event.StandardAdapterStartUpEvent</start-up-event-imp>
  <heartbeat-event-imp>com.adaptris.core.HeartbeatEvent</heartbeat-event-imp>
  <shared-components>
    <connections/>
    <services/>
  </shared-components>
  <event-handler class="default-event-handler">
    <unique-id>DefaultEventHandler</unique-id>
    <connection class="null-connection">
      <unique-id>agitated-einstein</unique-id>
    </connection>
    <producer class="null-message-producer">
      <unique-id>romantic-bartik</unique-id>
    </producer>
  </event-handler>
  <message-error-handler class="null-processing-exception-handler">
    <unique-id>silly-dijkstra</unique-id>
  </message-error-handler>
  <failed-message-retrier class="no-retries">
    <unique-id>jolly-ptolemy</unique-id>
  </failed-message-retrier>
  <channel-list>
    <channel>
      <consume-connection class="null-connection">
        <unique-id>romantic-galileo</unique-id>
      </consume-connection>
      <produce-connection class="null-connection">
        <unique-id>amazing-wiles</unique-id>
      </produce-connection>
      <workflow-list>
        <standard-workflow>
          <consumer class="fs-consumer">
            <unique-id>tender-allen</unique-id>
            <poller class="fixed-interval-poller">
              <poll-interval>
                <unit>SECONDS</unit>
                <interval>10</interval>
              </poll-interval>
            </poller>
            <create-dirs>true</create-dirs>
            <file-sorter class="fs-sort-none"/>
            <base-directory-url>./xml-in</base-directory-url>
            <wip-suffix>.wip</wip-suffix>
          </consumer>
          <service-collection class="service-list">
            <unique-id>naughty-lovelace</unique-id>
            <services>
              <xml-edi-stream-service>
                <unique-id>prickly-albattani</unique-id>
                <edi-separator-set class="edi-edifact-separators"/>
                <xml-tagnames class="edi-default-xml-tag-names"/>
                <xml-definition-url>classpath:edi96a_invoicegxml_ffxml.xml</xml-definition-url>
                <message-name>INVOIC</message-name>
                <message-version>D96A</message-version>
                <message-organisation>EDIFACT</message-organisation>
              </xml-edi-stream-service>
            </services>
          </service-collection>
          <producer class="fs-producer">
            <unique-id>admiring-lamarr</unique-id>
            <create-dirs>true</create-dirs>
            <fs-worker class="fs-nio-worker"/>
            <filename-creator class="formatted-filename-creator">
              <filename-format>%1$s</filename-format>
            </filename-creator>
            <base-directory-url>./edi-out</base-directory-url>
          </producer>
          <produce-exception-handler class="null-produce-exception-handler"/>
          <unique-id>nostalgic-murdock</unique-id>
          <message-metrics-interceptor>
            <unique-id>nostalgic-murdock-MessageMetrics</unique-id>
            <timeslice-duration>
              <unit>MINUTES</unit>
              <interval>5</interval>
            </timeslice-duration>
            <timeslice-history-count>12</timeslice-history-count>
          </message-metrics-interceptor>
          <in-flight-workflow-interceptor>
            <unique-id>nostalgic-murdock-InFlight</unique-id>
          </in-flight-workflow-interceptor>
        </standard-workflow>
        <standard-workflow>
          <consumer class="fs-consumer">
            <unique-id>tender-allen</unique-id>
            <poller class="fixed-interval-poller">
              <poll-interval>
                <unit>SECONDS</unit>
                <interval>10</interval>
              </poll-interval>
            </poller>
            <create-dirs>true</create-dirs>
            <file-sorter class="fs-sort-none"/>
            <base-directory-url>./edi-in</base-directory-url>
            <wip-suffix>.wip</wip-suffix>
          </consumer>
          <service-collection class="service-list">
            <unique-id>naughty-lovelace</unique-id>
            <services>
              <edi-xml-stream-service>
                <unique-id>infallible-montalcini</unique-id>
                <edi-separator-set class="edi-edifact-separators"/>
                <xml-tagnames class="edi-legacy-xml-tag-names"/>
                <xml-definition-url>classpath:edi96a_invoicegxml_ffxml.xml</xml-definition-url>
                <message-name>INVOIC</message-name>
                <message-version>D96A</message-version>
                <message-organisation>EDIFACT</message-organisation>
              </edi-xml-stream-service>
            </services>
          </service-collection>
          <producer class="fs-producer">
            <unique-id>admiring-lamarr</unique-id>
            <create-dirs>true</create-dirs>
            <fs-worker class="fs-nio-worker"/>
            <filename-creator class="formatted-filename-creator">
              <filename-format>%1$s</filename-format>
            </filename-creator>
            <base-directory-url>./xml-out</base-directory-url>
          </producer>
          <produce-exception-handler class="null-produce-exception-handler"/>
          <unique-id>nostalgic-murdock 2</unique-id>
          <message-metrics-interceptor>
            <unique-id>nostalgic-murdock-MessageMetrics</unique-id>
            <timeslice-duration>
              <unit>MINUTES</unit>
              <interval>5</interval>
            </timeslice-duration>
            <timeslice-history-count>12</timeslice-history-count>
          </message-metrics-interceptor>
          <in-flight-workflow-interceptor>
            <unique-id>nostalgic-murdock-InFlight</unique-id>
          </in-flight-workflow-interceptor>
        </standard-workflow>
      </workflow-list>
      <unique-id>suspicious-bartik</unique-id>
    </channel>
  </channel-list>
  <message-error-digester class="standard-message-error-digester">
    <unique-id>ErrorDigest</unique-id>
    <digest-max-size>100</digest-max-size>
  </message-error-digester>
</adapter>
